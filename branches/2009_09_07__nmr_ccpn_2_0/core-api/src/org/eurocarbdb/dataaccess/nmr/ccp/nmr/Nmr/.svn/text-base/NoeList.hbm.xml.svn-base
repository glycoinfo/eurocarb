<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping auto-import="false" package="ccp.api.nmr.Nmr">
  <joined-subclass
    name="NoeList"
    extends="ccp.api.nmr.Nmr.AbstractMeasurementList"
    table="NMR_NOELIST"
    schema="nmr" 
  >
  <meta attribute="implements">java.lang.Comparable</meta>
  <meta attribute="class-description">List of NOE measurements, homonuclear or heteronuclear.</meta>
  <key column="ABSTRACTMEASUREMENTLISTID" foreign-key="NMR_NOELIST_ABSTMEASLIST_FK"/>
  
  <property
    name="_noeValueType"
    access="field"
    type="java.lang.String" length="80"
    not-null="false"
  >
    <meta attribute="field-description">Type of Intensity measurement</meta>
    <meta attribute="default-value">null</meta>
    <column name="NOEVALUETYPE" sql-type="VARCHAR(80)" length="80" not-null="false"/>
  </property>
  
  <property
    name="_refDescription"
    access="field"
    type="java.lang.String" length="254"
    not-null="false"
  >
    <meta attribute="field-description">Description of what the reference value means and how it was generated.</meta>
    <meta attribute="default-value">null</meta>
    <column name="REFDESCRIPTION" sql-type="VARCHAR(254)" length="254" not-null="false"/>
  </property>
  
  <property
    name="_refValue"
    access="field"
    type="java.lang.Float"
    not-null="false"
  >
    <meta attribute="field-description">Reference value, used for calibration of NOEs</meta>
    <meta attribute="default-value">null</meta>
    <column name="REFVALUE" sql-type="FLOAT" not-null="false"/>
  </property>
  
  <property
    name="_sf"
    access="field"
    type="java.lang.Float"
    not-null="false"
  >
    <meta attribute="field-description">Spectrometer frequency in MHz at which NOE was measured.</meta>
    <meta attribute="default-value">null</meta>
    <column name="SF" sql-type="FLOAT" not-null="false"/>
  </property>
  
  <set 
    name="hbExperiments" 
    table="NMR_EXPERIMENT"
      schema="nmr" 
    inverse="true" 
    cascade="none"
  >
    <meta attribute="field-description">Experiments for which HomoNoeList is valid</meta>
    <meta attribute="base-name">hbExperiment</meta>
    <cache usage="read-write"/>
    <key column="NOELISTID"/>
    <one-to-many class="ccp.api.nmr.Nmr.Experiment"/>
  </set>
  
  <set 
    name="hbMeasurements" 
    table="NMR_NOE"
      schema="nmr" 
    inverse="true" 
    cascade="all"
  >
    <meta attribute="field-description">N/A</meta>
    <meta attribute="base-name">hbMeasurement</meta>
    <cache usage="read-write"/>
    <key column="PARENTLISTID"/>
    <one-to-many class="ccp.api.nmr.Nmr.Noe"/>
  </set>

  </joined-subclass>

</hibernate-mapping>
