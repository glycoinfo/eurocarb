<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping auto-import="false" package="memops.api.Implementation">
  <class
    name="ApplicationData"
    table="IMPL_APPLICATIONDATA"
    schema="nmr" 
  >
  <meta attribute="extends" inherit="false">memops.general.AbstractModelObject</meta>
  <meta attribute="implements">java.lang.Comparable</meta>
  <meta attribute="class-description">Stores program-specific data. Intended for application-specific extensions to the data model. ApplicationData objects should be used only by the application that wrote them originally.  ApplicationData objects all have an attribute 'value' holding the data value. The data type of the value differs between the different subclasses of ApplicationData</meta>
  <cache usage="read-write"/>

  <id
    name="_dbId"
    access="field"
    column="MEMOPSDATATYPEOBJECTID"
    type="java.lang.Long">
    <meta attribute="scope-set">protected</meta>
    <generator class="native"/>
  </id>
  
  <discriminator
  column="ApplicationData_TYPE"
  type="string"/>
  
  <property
    name="_application"
    access="field"
    type="java.lang.String" length="80"
    not-null="true"
  >
    <meta attribute="field-description">Name of application owning the data.</meta>
    <meta attribute="default-value">null</meta>
    <column name="APPLICATION" sql-type="VARCHAR(80)" length="80" not-null="true"/>
  </property>
  
  <property
    name="_keyword"
    access="field"
    type="java.lang.String" length="80"
    not-null="true"
  >
    <meta attribute="field-description">keyword or name of variable</meta>
    <meta attribute="default-value">null</meta>
    <column name="KEYWORD" sql-type="VARCHAR(80)" length="80" not-null="true"/>
  </property>
  
  <subclass
  name="AppDataString"
  discriminator-value="AppDataString">
  
    <property
      name="_value"
      access="field"
      type="java.lang.String"
      not-null="true"
    >
      <meta attribute="field-description">Data value</meta>
      <meta attribute="default-value">null</meta>
      <column name="AppDataString_VALUE" sql-type="TEXT" not-null="false"/>
    </property>
  </subclass>
  
  <subclass
  name="AppDataDouble"
  discriminator-value="AppDataDouble">
  
    <property
      name="_value"
      access="field"
      type="java.lang.Double"
      not-null="true"
    >
      <meta attribute="field-description">Data value</meta>
      <meta attribute="default-value">null</meta>
      <column name="AppDataDouble_VALUE" sql-type="FLOAT8" not-null="false"/>
    </property>
  </subclass>
  
  <subclass
  name="AppDataFloat"
  discriminator-value="AppDataFloat">
  
    <property
      name="_value"
      access="field"
      type="java.lang.Float"
      not-null="true"
    >
      <meta attribute="field-description">Data value</meta>
      <meta attribute="default-value">null</meta>
      <column name="AppDataFloat_VALUE" sql-type="FLOAT" not-null="false"/>
    </property>
  </subclass>
  
  <subclass
  name="AppDataLong"
  discriminator-value="AppDataLong">
  
    <property
      name="_value"
      access="field"
      type="java.lang.Long"
      not-null="true"
    >
      <meta attribute="field-description">Data value</meta>
      <meta attribute="default-value">null</meta>
      <column name="AppDataLong_VALUE" sql-type="INTEGER" not-null="false"/>
    </property>
  </subclass>
  
  <subclass
  name="AppDataInt"
  discriminator-value="AppDataInt">
  
    <property
      name="_value"
      access="field"
      type="java.lang.Integer"
      not-null="true"
    >
      <meta attribute="field-description">Data value</meta>
      <meta attribute="default-value">null</meta>
      <column name="AppDataInt_VALUE" sql-type="INTEGER" not-null="false"/>
    </property>
  </subclass>
  
  <subclass
  name="AppDataBoolean"
  discriminator-value="AppDataBoolean">
  
    <property
      name="_value"
      access="field"
      type="java.lang.Boolean"
      not-null="true"
    >
      <meta attribute="field-description">Data value</meta>
      <meta attribute="default-value">null</meta>
      <column name="AppDataBoolean_VALUE" sql-type="BOOLEAN" not-null="false"/>
    </property>
  </subclass>

  </class>

</hibernate-mapping>
