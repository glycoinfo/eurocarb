<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping auto-import="false" package="ccp.api.nmr.NmrEntry">
  <class
    name="Entry"
    table="ENTR_ENTRY"
    schema="nmr" 
  >
  <meta attribute="extends" inherit="false">memops.general.AbstractModelObject</meta>
  <meta attribute="implements">java.lang.Comparable</meta>
  <meta attribute="class-description">Deposition (entry) for deposition into BMRB (NB may be used also for other depositions?).</meta>
  <cache usage="read-write"/>

  <id
    name="_dbId"
    access="field"
    column="DATAOBJECTID"
    type="java.lang.Long">
    <meta attribute="scope-set">protected</meta>
    <generator class="native"/>
  </id>
  
  <many-to-one
    name="_topObject"
    access="field"
    column="TOPOBJECTID"
    class="ccp.api.nmr.NmrEntry.NmrEntryStore"
    not-null="true"
    unique="false"
    foreign-key="ENTR_ENTRY_NMRENTRSTOR_TO_FK"
  >
    <meta attribute="field-description">hard coded link to topObject</meta>
  </many-to-one>
  
  <property
    name="_bmrbProcessing"
    access="field"
    type="java.lang.String" length="254"
    not-null="false"
  >
    <meta attribute="field-description">BioMagResBank processing information</meta>
    <meta attribute="default-value">null</meta>
    <column name="BMRBPROCESSING" sql-type="VARCHAR(254)" length="254" not-null="false"/>
  </property>
  
  <property
    name="_details"
    access="field"
    type="java.lang.String"
    not-null="false"
  >
    <meta attribute="field-description">Free text, for notes, explanatory comments, etc.</meta>
    <meta attribute="default-value">null</meta>
    <column name="DETAILS" sql-type="TEXT" not-null="false"/>
  </property>
  
  <property
    name="_entryType"
    access="field"
    type="java.lang.String" length="80"
    not-null="true"
  >
    <meta attribute="field-description">Type of NMR study in the entry</meta>
    <meta attribute="default-value">"solution"</meta>
    <column name="ENTRYTYPE" sql-type="VARCHAR(80)" length="80" not-null="true"/>
  </property>
  
  <property
    name="_experimentListDetails"
    access="field"
    type="java.lang.String" length="254"
    not-null="false"
  >
    <meta attribute="field-description">Commens etc. to experiment list</meta>
    <meta attribute="default-value">null</meta>
    <column name="EXPERIMENTLISTDETAILS" sql-type="VARCHAR(254)" length="254" not-null="false"/>
  </property>
  
  <property
    name="_name"
    access="field"
    type="java.lang.String" length="80"
    not-null="true"
  >
    <meta attribute="field-description">name of Entry</meta>
    <meta attribute="default-value">null</meta>
    <column name="NAME" sql-type="VARCHAR(80)" length="80" not-null="true" unique-key="entryUniqueKey" />
  </property>
  
  <property
    name="_title"
    access="field"
    type="java.lang.String" length="254"
    not-null="false"
  >
    <meta attribute="field-description">Official title of Entry</meta>
    <meta attribute="default-value">null</meta>
    <column name="TITLE" sql-type="VARCHAR(254)" length="254" not-null="false"/>
  </property>
  
  <list
    name="hbKeywords"
    table="ENTR_ENTRY_KEYWORDS"
      schema="nmr" 
    cascade="all"
  >
    <meta attribute="field-description">Keywords for searching on Entry</meta>
    <cache usage="read-write" />
    <key column="ENTRYID" foreign-key="ENTR_ENTRY_KEYWORDS_FK"/>
    <index>
      <column name="order_" sql-type="int8" not-null="true"/>
    </index>
    <element type="java.lang.String" not-null="true">
      <column name="KEYWORD" sql-type="VARCHAR(80)" not-null="true"/>
    </element>
  </list>
  
  <list
    name="hbApplicationData"
    table="ENTR_ENTRY2APPLDATA"
      schema="nmr" 
    access="property"
    cascade="all"
  >
    <meta attribute="field-description">Application-specific data</meta>
    <key column="DATAOBJECTID"/>
    <list-index column="ORDER_applicationData_" base="1"/>
    <many-to-many
      column="APPLICATIONDATAID"
      class="memops.api.Implementation.ApplicationData"
      foreign-key="IMPL_DATAOBJECT_APPLDATA_FK"
      unique="true">
    </many-to-many>
  </list>
  
  <many-to-one
    name="hbAccess"
    column="ACCESSID"
    class="memops.api.AccessControl.AccessObject" 
    not-null="false"
    unique="true"
    foreign-key="IMPL_DATAOBJECT_ACCESS_FK" 
  >
    <meta attribute="field-description">Access object that controls object access</meta>
  </many-to-one>
  
  <many-to-one
    name="hbMolSystem"
    column="MOLSYSTEMID"
    class="ccp.api.molecule.MolSystem.MolSystem" 
    cascade="merge,save-update"
    not-null="false"
    unique="false"
    foreign-key="ENTR_ENTRY_MOLSYSTEM_FK" 
  >
    <meta attribute="field-description">MolSystem definign the entry</meta>
  </many-to-one>
  
  <many-to-one
    name="hbNmrEntryStore"
    column="NMRENTRYSTOREID"
    class="ccp.api.nmr.NmrEntry.NmrEntryStore" 
    cascade="merge,save-update"
    not-null="true"
    unique="false"
    foreign-key="ENTR_ENTRY_NMRENTRSTOR_FK" 
    unique-key="entryUniqueKey" 
  >
    <meta attribute="field-description">N/A</meta>
  </many-to-one>
  
  <many-to-one
    name="hbPrimaryCitation"
    column="PRIMARYCITATIONID"
    class="ccp.api.general.Citation.Citation" 
    not-null="false"
    unique="false"
    foreign-key="ENTR_ENTRY_PRIMCITA_FK" 
  >
    <meta attribute="field-description">Primary citation for BMRB entry</meta>
  </many-to-one>
  
  <many-to-one
    name="hbStudy"
    column="STUDYID"
    class="ccp.api.nmr.NmrEntry.Study" 
    cascade="merge,save-update"
    not-null="false"
    unique="false"
    foreign-key="ENTR_ENTRY_STUDY_FK" 
  >
    <meta attribute="field-description">Study that Entry is part of</meta>
  </many-to-one>
  
  <set 
    name="hbEntryMolecules" 
    table="ENTR_ENTRYMOLECULE"
      schema="nmr" 
    inverse="true" 
    cascade="all"
  >
    <meta attribute="field-description">N/A</meta>
    <meta attribute="base-name">hbEntryMolecule</meta>
    <cache usage="read-write"/>
    <key column="ENTRYID"/>
    <one-to-many class="ccp.api.nmr.NmrEntry.EntryMolecule"/>
  </set>
  
  <set 
    name="hbRelatedEntries" 
    table="ENTR_RELATEDENTRY"
      schema="nmr" 
    inverse="true" 
    cascade="all"
  >
    <meta attribute="field-description">N/A</meta>
    <meta attribute="base-name">hbRelatedEntry</meta>
    <cache usage="read-write"/>
    <key column="ENTRYID"/>
    <one-to-many class="ccp.api.nmr.NmrEntry.RelatedEntry"/>
  </set>
  
  <list
    name="hbAuthors" 
    table="ENTR_ENTRY2AUTHORS"
      schema="nmr" 
    inverse="false" 
    cascade="none"
  >
    <meta attribute="field-description">Authors of entry.</meta>
    <meta attribute="base-name">hbAuthor</meta>
    <cache usage="read-write"/>
    <key column="AUTHOREDENTRYID" foreign-key="AFFI_PERSON_AUTHENTR_FK" not-null="true"/>
    <list-index column="order_authors_" base="1"/>
    <many-to-many column="AUTHORID" class="ccp.api.general.Affiliation.Person" foreign-key="ENTR_ENTRY_AUTHORS_FK"/>
  </list>
  
  <set 
    name="hbContactPersons" 
    table="ENTR_ENTRY2CONTPERS"
      schema="nmr" 
    inverse="false" 
    cascade="none"
  >
    <meta attribute="field-description">Contact persons for entry</meta>
    <meta attribute="base-name">hbContactPerson</meta>
    <cache usage="read-write"/>
    <key column="CONTACTENTRYID" foreign-key="AFFI_PERSON_CONTENTR_FK"/>
    <many-to-many column="CONTACTPERSONID" class="ccp.api.general.Affiliation.Person" foreign-key="ENTR_ENTRY_CONTPERS_FK"/>
  </set>
  
  <set 
    name="hbDerivedDataLists" 
    table="ENTR_ENTRY2DERIDATALIST"
      schema="nmr" 
    inverse="false" 
    cascade="none"
  >
    <meta attribute="field-description">Derived Nmr data lists relevant for Entry</meta>
    <meta attribute="base-name">hbDerivedDataList</meta>
    <cache usage="read-write"/>
    <key column="ENTRYID" foreign-key="NMR_DERIDATALIST_ENTRIES_FK"/>
    <many-to-many column="DERIVEDDATALISTID" class="ccp.api.nmr.Nmr.DerivedDataList" foreign-key="ENTR_ENTRY_DERIDATALIST_FK"/>
  </set>
  
  <set 
    name="hbExperiments" 
    table="ENTR_ENTRY2EXPE"
      schema="nmr" 
    inverse="false" 
    cascade="none"
  >
    <meta attribute="field-description">Nmr Experiments included in Entry</meta>
    <meta attribute="base-name">hbExperiment</meta>
    <cache usage="read-write"/>
    <key column="ENTRYID" foreign-key="NMR_EXPERIMENT_ENTRIES_FK"/>
    <many-to-many column="EXPERIMENTID" class="ccp.api.nmr.Nmr.Experiment" foreign-key="ENTR_ENTRY_EXPE_FK"/>
  </set>
  
  <set 
    name="hbLaboratories" 
    table="ENTR_ENTRY2LABO"
      schema="nmr" 
    inverse="false" 
    cascade="none"
  >
    <meta attribute="field-description">Research groups originating the entry</meta>
    <meta attribute="base-name">hbLaboratory</meta>
    <cache usage="read-write"/>
    <key column="NMRENTRYID" foreign-key="AFFI_GROUP_NMRENTRIES_FK"/>
    <many-to-many column="LABORATORYID" class="ccp.api.general.Affiliation.Group" foreign-key="ENTR_ENTRY_LABO_FK"/>
  </set>
  
  <set 
    name="hbMeasurementLists" 
    table="ENTR_ENTRY2MEASLIST"
      schema="nmr" 
    inverse="false" 
    cascade="none"
  >
    <meta attribute="field-description">Measurement lists included  in Entry</meta>
    <meta attribute="base-name">hbMeasurementList</meta>
    <cache usage="read-write"/>
    <key column="ENTRYID" foreign-key="NMR_ABSTMEASLIST_ENTRIES_FK"/>
    <many-to-many column="MEASUREMENTLISTID" class="ccp.api.nmr.Nmr.AbstractMeasurementList" foreign-key="ENTR_ENTRY_MEASLIST_FK"/>
  </set>
  
  <set 
    name="hbOtherCitations" 
    table="ENTR_ENTRY2OTHECITA"
      schema="nmr" 
    inverse="false" 
    cascade="none"
  >
    <meta attribute="field-description">Other (non-primary) citations for entry</meta>
    <meta attribute="base-name">hbOtherCitation</meta>
    <cache usage="read-write"/>
    <key column="NMRENTRYID" foreign-key="CITA_CITATION_NMRENTRIES_FK"/>
    <many-to-many column="OTHERCITATIONID" class="ccp.api.general.Citation.Citation" foreign-key="ENTR_ENTRY_OTHECITA_FK"/>
  </set>
  
  <set 
    name="hbPeakLists" 
    table="ENTR_ENTRY2PEAKLISTS"
      schema="nmr" 
    inverse="false" 
    cascade="none"
  >
    <meta attribute="field-description">Nmr.Peaklists that are part of Entry</meta>
    <meta attribute="base-name">hbPeakList</meta>
    <cache usage="read-write"/>
    <key column="ENTRYID" foreign-key="NMR_PEAKLIST_ENTRIES_FK"/>
    <many-to-many column="PEAKLISTID" class="ccp.api.nmr.Nmr.PeakList" foreign-key="ENTR_ENTRY_PEAKLISTS_FK"/>
  </set>
  
  <set 
    name="hbSoftware" 
    table="ENTR_ENTRY2SOFTWARE"
      schema="nmr" 
    inverse="false" 
    cascade="none"
  >
    <meta attribute="field-description">software used for analysis in entry</meta>
    <meta attribute="base-name">hbSoftware</meta>
    <cache usage="read-write"/>
    <key column="ENTRYID" foreign-key="METH_SOFTWARE_ENTRIES_FK"/>
    <many-to-many column="SOFTWAREID" class="ccp.api.general.Method.Software" foreign-key="ENTR_ENTRY_SOFTWARE_FK"/>
  </set>
  
  <set 
    name="hbStructureAnalyses" 
    table="ENTR_ENTRY2STRUANAL"
      schema="nmr" 
    inverse="false" 
    cascade="none"
  >
    <meta attribute="field-description">StructureAnalyses being deposited in entry.</meta>
    <meta attribute="base-name">hbStructureAnalysis</meta>
    <cache usage="read-write"/>
    <key column="ENTRYID" foreign-key="NMR_STRUANAL_ENTRIES_FK"/>
    <many-to-many column="STRUCTUREANALYSISID" class="ccp.api.nmr.Nmr.StructureAnalysis" foreign-key="ENTR_ENTRY_STRUANAL_FK"/>
  </set>
  
  <set 
    name="hbStructureGenerations" 
    table="ENTR_ENTRY2STRUGENE"
      schema="nmr" 
    inverse="false" 
    cascade="none"
  >
    <meta attribute="field-description">StructureGenerations being deposited in entry.</meta>
    <meta attribute="base-name">hbStructureGeneration</meta>
    <cache usage="read-write"/>
    <key column="ENTRYID" foreign-key="NMR_STRUGENE_ENTRIES_FK"/>
    <many-to-many column="STRUCTUREGENERATIONID" class="ccp.api.nmr.Nmr.StructureGeneration" foreign-key="ENTR_ENTRY_STRUGENE_FK"/>
  </set>
  
  <set 
    name="hbStructureGroups" 
    table="ENTR_ENTRY2STRUGROU"
      schema="nmr" 
    inverse="false" 
    cascade="none"
  >
    <meta attribute="field-description">Groups of structures included in Entry.</meta>
    <meta attribute="base-name">hbStructureGroup</meta>
    <cache usage="read-write"/>
    <key column="ENTRYID" foreign-key="MOLS_STRUGROU_ENTRIES_FK"/>
    <many-to-many column="STRUCTUREGROUPID" class="ccp.api.molecule.MolSystem.StructureGroup" foreign-key="ENTR_ENTRY_STRUGROU_FK"/>
  </set>

  </class>

</hibernate-mapping>
