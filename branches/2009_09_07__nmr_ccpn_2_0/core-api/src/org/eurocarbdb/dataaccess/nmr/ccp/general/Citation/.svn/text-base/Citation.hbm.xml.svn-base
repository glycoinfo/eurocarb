<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping auto-import="false" package="ccp.api.general.Citation">
  <class
    name="Citation"
    table="CITA_CITATION"
    schema="nmr" 
  >
  <meta attribute="extends" inherit="false">memops.general.AbstractModelObject</meta>
  <meta attribute="implements">java.lang.Comparable</meta>
  <meta attribute="class-description">Citation (litterature reference).</meta>
  <cache usage="read-write"/>

  <id
    name="_dbId"
    access="field"
    column="DATAOBJECTID"
    type="java.lang.Long">
    <meta attribute="scope-set">protected</meta>
    <generator class="native"/>
  </id>
  
  <many-to-one
    name="_topObject"
    access="field"
    column="TOPOBJECTID"
    class="ccp.api.general.Citation.CitationStore"
    not-null="true"
    unique="false"
    foreign-key="CITA_CITATION_CITASTOR_TO_FK"
  >
    <meta attribute="field-description">hard coded link to topObject</meta>
  </many-to-one>
  
  <property
    name="_casAbstractCode"
    access="field"
    type="java.lang.String" length="80"
    not-null="false"
  >
    <meta attribute="field-description">Chemical Abstracts abstracting service (CAS) code</meta>
    <meta attribute="default-value">null</meta>
    <column name="CASABSTRACTCODE" sql-type="VARCHAR(80)" length="80" not-null="false"/>
  </property>
  
  <property
    name="_details"
    access="field"
    type="java.lang.String" length="254"
    not-null="false"
  >
    <meta attribute="field-description">Free text, for notes, explanatory comments, etc.</meta>
    <meta attribute="default-value">null</meta>
    <column name="DETAILS" sql-type="VARCHAR(254)" length="254" not-null="false"/>
  </property>
  
  <property
    name="_doi"
    access="field"
    type="java.lang.String" length="254"
    not-null="false"
  >
    <meta attribute="field-description">DOI identifier of citation</meta>
    <meta attribute="default-value">null</meta>
    <column name="DOI" sql-type="VARCHAR(254)" length="254" not-null="false"/>
  </property>
  
  <property
    name="_firstPage"
    access="field"
    type="java.lang.String" length="80"
    not-null="false"
  >
    <meta attribute="field-description">First page number (not an integer, as it might be e.g. '235A')</meta>
    <meta attribute="default-value">null</meta>
    <column name="FIRSTPAGE" sql-type="VARCHAR(80)" length="80" not-null="false"/>
  </property>
  
  <property
    name="_lastPage"
    access="field"
    type="java.lang.String" length="80"
    not-null="false"
  >
    <meta attribute="field-description">Last page number (not an integer, as it might be e.g. '238A')</meta>
    <meta attribute="default-value">null</meta>
    <column name="LASTPAGE" sql-type="VARCHAR(80)" length="80" not-null="false"/>
  </property>
  
  <property
    name="_medlineUiCode"
    access="field"
    type="java.lang.String" length="80"
    not-null="false"
  >
    <meta attribute="field-description">Medline UI index code</meta>
    <meta attribute="default-value">null</meta>
    <column name="MEDLINEUICODE" sql-type="VARCHAR(80)" length="80" not-null="false"/>
  </property>
  
  <property
    name="_pubMedId"
    access="field"
    type="java.lang.String" length="80"
    not-null="false"
  >
    <meta attribute="field-description">pubMed ID</meta>
    <meta attribute="default-value">null</meta>
    <column name="PUBMEDID" sql-type="VARCHAR(80)" length="80" not-null="false"/>
  </property>
  
  <property
    name="_serial"
    access="field"
    type="java.lang.Integer"
    not-null="true"
  >
    <meta attribute="field-description">Serial number of object. Serves as object main key. Serial numbers of deleted objects are not re-used. Serial numbers can only be set by the implementation. Values are in practice always positive, since negative values are interpreted as a signal to set the next free serial</meta>
    <meta attribute="default-value">null</meta>
    <column name="SERIAL" sql-type="INTEGER" not-null="true" unique-key="citationUniqueKey" />
  </property>
  
  <property
    name="_status"
    access="field"
    type="java.lang.String" length="80"
    not-null="false"
  >
    <meta attribute="field-description">E.g. 'published', 'in press','to be published' ?</meta>
    <meta attribute="default-value">null</meta>
    <column name="STATUS" sql-type="VARCHAR(80)" length="80" not-null="false"/>
  </property>
  
  <property
    name="_title"
    access="field"
    type="java.lang.String"
    not-null="false"
  >
    <meta attribute="field-description">Citation (article) Title</meta>
    <meta attribute="default-value">null</meta>
    <column name="TITLE" sql-type="TEXT" not-null="false"/>
  </property>
  
  <property
    name="_year"
    access="field"
    type="java.lang.Integer"
    not-null="false"
  >
    <meta attribute="field-description">Year of publication</meta>
    <meta attribute="default-value">null</meta>
    <column name="YEAR" sql-type="INTEGER" not-null="false"/>
  </property>
  
  <list
    name="hbKeywords"
    table="CITA_CITATION_KEYWORDS"
      schema="nmr" 
    cascade="all"
  >
    <meta attribute="field-description">Keywords characterising Citation</meta>
    <cache usage="read-write" />
    <key column="CITATIONID" foreign-key="CITA_CITATION_KEYWORDS_FK"/>
    <index>
      <column name="order_" sql-type="int8" not-null="true"/>
    </index>
    <element type="java.lang.String" not-null="true">
      <column name="KEYWORD" sql-type="VARCHAR(80)" not-null="true"/>
    </element>
  </list>
  
  <list
    name="hbApplicationData"
    table="CITA_CITATION2APPLDATA"
      schema="nmr" 
    access="property"
    cascade="all"
  >
    <meta attribute="field-description">Application-specific data</meta>
    <key column="DATAOBJECTID"/>
    <list-index column="ORDER_applicationData_" base="1"/>
    <many-to-many
      column="APPLICATIONDATAID"
      class="memops.api.Implementation.ApplicationData"
      foreign-key="IMPL_DATAOBJECT_APPLDATA_FK"
      unique="true">
    </many-to-many>
  </list>
  
  <many-to-one
    name="hbAccess"
    column="ACCESSID"
    class="memops.api.AccessControl.AccessObject" 
    not-null="false"
    unique="true"
    foreign-key="IMPL_DATAOBJECT_ACCESS_FK" 
  >
    <meta attribute="field-description">Access object that controls object access</meta>
  </many-to-one>
  
  <many-to-one
    name="hbCitationStore"
    column="CITATIONSTOREID"
    class="ccp.api.general.Citation.CitationStore" 
    cascade="merge,save-update"
    not-null="true"
    unique="false"
    foreign-key="CITA_CITATION_CITASTOR_FK" 
    unique-key="citationUniqueKey" 
  >
    <meta attribute="field-description">N/A</meta>
  </many-to-one>
  
  <set 
    name="hbShiftReferences" 
    table="NMR_SHIFTREFERENCE"
      schema="nmr" 
    inverse="true" 
    cascade="none"
  >
    <meta attribute="field-description">Shift References citing CItation.</meta>
    <meta attribute="base-name">hbShiftReference</meta>
    <cache usage="read-write"/>
    <key column="CITATIONID"/>
    <one-to-many class="ccp.api.nmr.Nmr.ShiftReference"/>
  </set>
  
  <set 
    name="hbRatioShiftReferences" 
    table="NMR_SHIFTREFERENCE"
      schema="nmr" 
    inverse="true" 
    cascade="none"
  >
    <meta attribute="field-description">Shift reference indirect shift ratio references.</meta>
    <meta attribute="base-name">hbRatioShiftReference</meta>
    <cache usage="read-write"/>
    <key column="SHIFTRATIOCITATIONID"/>
    <one-to-many class="ccp.api.nmr.Nmr.ShiftReference"/>
  </set>
  
  <list 
    name="hbNmrEntriesPrimary" 
    table="ENTR_ENTRY"
      schema="nmr" 
    inverse="true" 
    cascade="none"
  >
    <meta attribute="field-description">NMR entries that have this as their primary citation</meta>
    <meta attribute="base-name">hbNmrEntryPrimary</meta>
    <cache usage="read-write"/>
    <key column="PRIMARYCITATIONID"/>
    <list-index column="order_nmrEntriesPrimary_" base="1"/>
    <one-to-many class="ccp.api.nmr.NmrEntry.Entry"/>
  </list>
  
  <list
    name="hbAuthors" 
    table="CITA_CITATION2AUTHORS"
      schema="nmr" 
    inverse="false" 
    cascade="none"
  >
    <meta attribute="field-description">Authors of Citation</meta>
    <meta attribute="base-name">hbAuthor</meta>
    <cache usage="read-write"/>
    <key column="AUTHORCITATIONID" foreign-key="AFFI_PERSON_AUTHCITA_FK" not-null="true"/>
    <list-index column="order_authors_" base="1"/>
    <many-to-many column="AUTHORID" class="ccp.api.general.Affiliation.Person" foreign-key="CITA_CITATION_AUTHORS_FK"/>
  </list>
  
  <list
    name="hbEditors" 
    table="CITA_CITATION2EDITORS"
      schema="nmr" 
    inverse="false" 
    cascade="none"
  >
    <meta attribute="field-description">People who are editors of Citation</meta>
    <meta attribute="base-name">hbEditor</meta>
    <cache usage="read-write"/>
    <key column="EDITORCITATIONID" foreign-key="AFFI_PERSON_EDITCITA_FK" not-null="true"/>
    <list-index column="order_editors_" base="1"/>
    <many-to-many column="EDITORID" class="ccp.api.general.Affiliation.Person" foreign-key="CITA_CITATION_EDITORS_FK"/>
  </list>
  
  <set 
    name="hbNmrProbes" 
    table="INST_NMRPROBE2CITATIONS"
      schema="nmr" 
    inverse="true" 
    cascade="none"
  >
    <meta attribute="field-description">Nmr probes that use this Citation</meta>
    <meta attribute="base-name">hbNmrProbe</meta>
    <cache usage="read-write"/>
    <key column="CITATIONID" foreign-key="INST_NMRPROBE_CITATIONS_FK"/>
    <many-to-many column="NMRPROBEID" class="ccp.api.general.Instrument.NmrProbe" foreign-key="CITA_CITATION_NMRPROBES_FK"/>
  </set>
  
  <set 
    name="hbNmrSpectrometers" 
    table="INST_NMRSPEC2CITATIONS"
      schema="nmr" 
    inverse="true" 
    cascade="none"
  >
    <meta attribute="field-description">Nmr Spectrometers citing this Citation</meta>
    <meta attribute="base-name">hbNmrSpectrometer</meta>
    <cache usage="read-write"/>
    <key column="CITATIONID" foreign-key="INST_NMRSPEC_CITATIONS_FK"/>
    <many-to-many column="NMRSPECTROMETERID" class="ccp.api.general.Instrument.NmrSpectrometer" foreign-key="CITA_CITATION_NMRSPEC_FK"/>
  </set>
  
  <set 
    name="hbMethods" 
    table="METH_METHOD2CITATIONS"
      schema="nmr" 
    inverse="true" 
    cascade="none"
  >
    <meta attribute="field-description">Methods described by Citation</meta>
    <meta attribute="base-name">hbMethod</meta>
    <cache usage="read-write"/>
    <key column="CITATIONID" foreign-key="METH_METHOD_CITATIONS_FK"/>
    <many-to-many column="METHODID" class="ccp.api.general.Method.Method" foreign-key="CITA_CITATION_METHODS_FK"/>
  </set>
  
  <set 
    name="hbSoftware" 
    table="METH_SOFTWARE2CITATIONS"
      schema="nmr" 
    inverse="true" 
    cascade="none"
  >
    <meta attribute="field-description">Software described by Citation</meta>
    <meta attribute="base-name">hbSoftware</meta>
    <cache usage="read-write"/>
    <key column="CITATIONID" foreign-key="METH_SOFTWARE_CITATIONS_FK"/>
    <many-to-many column="SOFTWAREID" class="ccp.api.general.Method.Software" foreign-key="CITA_CITATION_SOFTWARE_FK"/>
  </set>
  
  <set 
    name="hbMolSystems" 
    table="MOLS_MOLSYSTEM2CITATIONS"
      schema="nmr" 
    inverse="true" 
    cascade="none"
  >
    <meta attribute="field-description">MolSystems described by Citation</meta>
    <meta attribute="base-name">hbMolSystem</meta>
    <cache usage="read-write"/>
    <key column="CITATIONID" foreign-key="MOLS_MOLSYSTEM_CITATIONS_FK"/>
    <many-to-many column="MOLSYSTEMID" class="ccp.api.molecule.MolSystem.MolSystem" foreign-key="CITA_CITATION_MOLSYSTEMS_FK"/>
  </set>
  
  <set 
    name="hbMolecules" 
    table="MOLE_MOLECULE2CITATIONS"
      schema="nmr" 
    inverse="true" 
    cascade="none"
  >
    <meta attribute="field-description">Molecules described by Citation</meta>
    <meta attribute="base-name">hbMolecule</meta>
    <cache usage="read-write"/>
    <key column="CITATIONID" foreign-key="MOLE_MOLECULE_CITATIONS_FK"/>
    <many-to-many column="MOLECULEID" class="ccp.api.molecule.Molecule.Molecule" foreign-key="CITA_CITATION_MOLECULES_FK"/>
  </set>
  
  <set 
    name="hbDataLists" 
    table="NMR_DATALIST2CITATIONS"
      schema="nmr" 
    inverse="true" 
    cascade="none"
  >
    <meta attribute="field-description">Nmr Generic Measurement Lists citing Citation.</meta>
    <meta attribute="base-name">hbDataList</meta>
    <cache usage="read-write"/>
    <key column="CITATIONID" foreign-key="NMR_DATALIST_CITATIONS_FK"/>
    <many-to-many column="DATALISTID" class="ccp.api.nmr.Nmr.DataList" foreign-key="CITA_CITATION_DATALISTS_FK"/>
  </set>
  
  <set 
    name="hbExperiments" 
    table="NMR_EXPERIMENT2CITATIONS"
      schema="nmr" 
    inverse="true" 
    cascade="none"
  >
    <meta attribute="field-description">Nmr Experiments referring to Citation</meta>
    <meta attribute="base-name">hbExperiment</meta>
    <cache usage="read-write"/>
    <key column="CITATIONID" foreign-key="NMR_EXPERIMENT_CITATIONS_FK"/>
    <many-to-many column="EXPERIMENTID" class="ccp.api.nmr.Nmr.Experiment" foreign-key="CITA_CITATION_EXPE_FK"/>
  </set>
  
  <set 
    name="hbSampleConditionSets" 
    table="NMR_SAMPCONDSET2CITATIONS"
      schema="nmr" 
    inverse="true" 
    cascade="none"
  >
    <meta attribute="field-description">SampleConditionSets quoting Citation</meta>
    <meta attribute="base-name">hbSampleConditionSet</meta>
    <cache usage="read-write"/>
    <key column="CITATIONID" foreign-key="NMR_SAMPCONDSET_CITATIONS_FK"/>
    <many-to-many column="SAMPLECONDITIONSETID" class="ccp.api.nmr.Nmr.SampleConditionSet" foreign-key="CITA_CITATION_SAMPCONDSETS_FK"/>
  </set>
  
  <set 
    name="hbNmrEntries" 
    table="ENTR_ENTRY2OTHECITA"
      schema="nmr" 
    inverse="true" 
    cascade="none"
  >
    <meta attribute="field-description">BioMagResBank entries that cite Citatoin as non-primary reference</meta>
    <meta attribute="base-name">hbNmrEntry</meta>
    <cache usage="read-write"/>
    <key column="OTHERCITATIONID" foreign-key="ENTR_ENTRY_OTHECITA_FK"/>
    <many-to-many column="NMRENTRYID" class="ccp.api.nmr.NmrEntry.Entry" foreign-key="CITA_CITATION_NMRENTRIES_FK"/>
  </set>

  </class>

</hibernate-mapping>
