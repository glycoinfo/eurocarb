<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping auto-import="false" package="ccp.api.molecule.ChemComp">
  <class
    name="ChemComp"
    table="CHEM_CHEMCOMP"
    schema="nmr" 
  >
  <meta attribute="extends" inherit="false">memops.general.AbstractModelObject</meta>
  <meta attribute="implements">java.lang.Comparable</meta>
  <meta attribute="class-description">Chemical Component. E.g. GLU (glutamate). Describes the topology of templates that serve as building block for molecules. The ChemComp holds information valid for all toploigical variants, as well as all Atoms, Bonds, Angles etc. for the different ChemCompVars (e.g. 'start','protonated'; 'end','deprotonated', etc.). For this reason the Atoms, Bonds etc. linked to do not form a consistent set. </meta>
  <cache usage="read-write"/>

  <id
    name="_dbId"
    access="field"
    column="TOPOBJECTID"
    type="java.lang.Long">
    <meta attribute="scope-set">protected</meta>
    <generator class="native"/>
  </id>
  
  <property
    name="_segment"
    access="field"
    type="java.lang.String" length="20"
    not-null="false"
  >
    <meta attribute="field-description"></meta>
    <column name="SEGMENT" sql-type="VARCHAR(20)" length="20" not-null="false"  unique-key="chemCompUniqueKey" />
  </property>
  
  <property
    name="_guid"
    access="field"
    type="java.lang.String" length="80"
    not-null="true"
  >
    <meta attribute="field-description">global unique identifier - set by implementation.</meta>
    <meta attribute="default-value">null</meta>
    <column name="GUID" sql-type="VARCHAR(80)" length="80" not-null="true"/>
  </property>
  
  <property
    name="_baseGlycoCtCode"
    access="field"
    type="java.lang.String"
    not-null="false"
  >
    <meta attribute="field-description">base glycoCT description code. For carbohydrates. The full glycoCtCode is only defined at the ChemCompVar level, but htis gives the base code that is exanded on in the vars.</meta>
    <meta attribute="default-value">null</meta>
    <column name="BASEGLYCOCTCODE" sql-type="TEXT" not-null="false"/>
  </property>
  
  <property
    name="_beilsteinCode"
    access="field"
    type="java.lang.String" length="32"
    not-null="false"
  >
    <meta attribute="field-description">Beilstein compound code</meta>
    <meta attribute="default-value">null</meta>
    <column name="BEILSTEINCODE" sql-type="VARCHAR(32)" length="32" not-null="false"/>
  </property>
  
  <property
    name="_casRegCode"
    access="field"
    type="java.lang.String" length="32"
    not-null="false"
  >
    <meta attribute="field-description">CAS code</meta>
    <meta attribute="default-value">null</meta>
    <column name="CASREGCODE" sql-type="VARCHAR(32)" length="32" not-null="false"/>
  </property>
  
  <property
    name="_ccpCode"
    access="field"
    type="java.lang.String" length="80"
    not-null="true"
  >
    <meta attribute="field-description">ccp Code of ChemComp. Part of the class key</meta>
    <meta attribute="default-value">null</meta>
    <column name="CCPCODE" sql-type="VARCHAR(80)" length="80" not-null="true" unique-key="chemCompUniqueKey" />
  </property>
  
  <property
    name="_code3Letter"
    access="field"
    type="java.lang.String" length="3"
    not-null="false"
  >
    <meta attribute="field-description">PDB three letter code of ChemComp.</meta>
    <meta attribute="default-value">null</meta>
    <column name="CODE3LETTER" sql-type="VARCHAR(3)" length="3" not-null="false"/>
  </property>
  
  <property
    name="_details"
    access="field"
    type="java.lang.String" length="254"
    not-null="false"
  >
    <meta attribute="field-description">Free text, for notes, explanatory comments, etc.</meta>
    <meta attribute="default-value">null</meta>
    <column name="DETAILS" sql-type="VARCHAR(254)" length="254" not-null="false"/>
  </property>
  
  <property
    name="_hasStdChirality"
    access="field"
    type="java.lang.Boolean"
    not-null="false"
  >
    <meta attribute="field-description">Does ChemComp have standard chirality? Serves to flag molecules containing ChemComps with non-standard chirality, such as D amino acids. Note that the attribute may not be set, for cases where no standard is defined.</meta>
    <meta attribute="default-value">null</meta>
    <column name="HASSTDCHIRALITY" sql-type="BOOLEAN" not-null="false"/>
  </property>
  
  <property
    name="_merckCode"
    access="field"
    type="java.lang.String" length="32"
    not-null="false"
  >
    <meta attribute="field-description">Merck compound code</meta>
    <meta attribute="default-value">null</meta>
    <column name="MERCKCODE" sql-type="VARCHAR(32)" length="32" not-null="false"/>
  </property>
  
  <property
    name="_molType"
    access="field"
    type="java.lang.String" length="32"
    not-null="true"
  >
    <meta attribute="field-description">molecule Type of ChemComp (part of the class key)</meta>
    <meta attribute="default-value">null</meta>
    <column name="MOLTYPE" sql-type="VARCHAR(32)" length="32" not-null="true" unique-key="chemCompUniqueKey" />
  </property>
  
  <property
    name="_name"
    access="field"
    type="java.lang.String" length="254"
    not-null="false"
  >
    <meta attribute="field-description">name of AbstractChemComp</meta>
    <meta attribute="default-value">null</meta>
    <column name="NAME" sql-type="VARCHAR(254)" length="254" not-null="false"/>
  </property>
  
  <property
    name="_sigmaAldrichCode"
    access="field"
    type="java.lang.String" length="32"
    not-null="false"
  >
    <meta attribute="field-description">SigmaAldrich compound code</meta>
    <meta attribute="default-value">null</meta>
    <column name="SIGMAALDRICHCODE" sql-type="VARCHAR(32)" length="32" not-null="false"/>
  </property>
  
  <list
    name="hbCommonNames"
    table="CHEM_CHEMCOMP_COMMNAME"
      schema="nmr" 
    cascade="all"
  >
    <meta attribute="field-description">Common names of ChemComp</meta>
    <cache usage="read-write" />
    <key column="CHEMCOMPID" foreign-key="CHEM_CHEMCOMP_COMMNAME_FK"/>
    <index>
      <column name="order_" sql-type="int8" not-null="true"/>
    </index>
    <element type="java.lang.String" not-null="true">
      <column name="COMMONNAME" sql-type="VARCHAR(254)" not-null="true"/>
    </element>
  </list>
  
  <list
    name="hbKeywords"
    table="CHEM_CHEMCOMP_KEYWORDS"
      schema="nmr" 
    cascade="all"
  >
    <meta attribute="field-description">Keywords for searching ChemComp. E.g. 'saccharide', ...</meta>
    <cache usage="read-write" />
    <key column="CHEMCOMPID" foreign-key="CHEM_CHEMCOMP_KEYWORDS_FK"/>
    <index>
      <column name="order_" sql-type="int8" not-null="true"/>
    </index>
    <element type="java.lang.String" not-null="true">
      <column name="KEYWORD" sql-type="VARCHAR(80)" not-null="true"/>
    </element>
  </list>
  
  <list
    name="hbApplicationData"
    table="CHEM_CHEMCOMP2APPLDATA"
      schema="nmr" 
    access="property"
    cascade="all"
  >
    <meta attribute="field-description">Application-specific data</meta>
    <key column="DATAOBJECTID"/>
    <list-index column="ORDER_applicationData_" base="1"/>
    <many-to-many
      column="APPLICATIONDATAID"
      class="memops.api.Implementation.ApplicationData"
      foreign-key="IMPL_DATAOBJECT_APPLDATA_FK"
      unique="true">
    </many-to-many>
  </list>
  
  <many-to-one
    name="hbAccess"
    column="ACCESSID"
    class="memops.api.AccessControl.AccessObject" 
    not-null="false"
    unique="true"
    foreign-key="IMPL_DATAOBJECT_ACCESS_FK" 
  >
    <meta attribute="field-description">Access object that controls object access</meta>
  </many-to-one>
  
  <set 
    name="hbChemAngles" 
    table="CHEM_CHEMANGLE"
      schema="nmr" 
    inverse="true" 
    cascade="all"
  >
    <meta attribute="field-description">N/A</meta>
    <meta attribute="base-name">hbChemAngle</meta>
    <cache usage="read-write"/>
    <key column="CHEMCOMPID"/>
    <one-to-many class="ccp.api.molecule.ChemComp.ChemAngle"/>
  </set>
  
  <set 
    name="hbChemAtomSets" 
    table="CHEM_CHEMATOMSET"
      schema="nmr" 
    inverse="true" 
    cascade="all"
  >
    <meta attribute="field-description">N/A</meta>
    <meta attribute="base-name">hbChemAtomSet</meta>
    <cache usage="read-write"/>
    <key column="CHEMCOMPID"/>
    <one-to-many class="ccp.api.molecule.ChemComp.ChemAtomSet"/>
  </set>
  
  <set 
    name="hbChemAtoms" 
    table="CHEM_ABSTRACTCHEMATOM"
      schema="nmr" 
    inverse="true" 
    cascade="all"
  >
    <meta attribute="field-description">N/A</meta>
    <meta attribute="base-name">hbChemAtom</meta>
    <cache usage="read-write"/>
    <key column="CHEMCOMPID"/>
    <one-to-many class="ccp.api.molecule.ChemComp.AbstractChemAtom"/>
  </set>
  
  <set 
    name="hbChemBonds" 
    table="CHEM_CHEMBOND"
      schema="nmr" 
    inverse="true" 
    cascade="all"
  >
    <meta attribute="field-description">N/A</meta>
    <meta attribute="base-name">hbChemBond</meta>
    <cache usage="read-write"/>
    <key column="CHEMCOMPID"/>
    <one-to-many class="ccp.api.molecule.ChemComp.ChemBond"/>
  </set>
  
  <set 
    name="hbChemCompVars" 
    table="CHEM_CHEMCOMPVAR"
      schema="nmr" 
    inverse="true" 
    cascade="all"
  >
    <meta attribute="field-description">N/A</meta>
    <meta attribute="base-name">hbChemCompVar</meta>
    <cache usage="read-write"/>
    <key column="CHEMCOMPID"/>
    <one-to-many class="ccp.api.molecule.ChemComp.ChemCompVar"/>
  </set>
  
  <set 
    name="hbChemTorsions" 
    table="CHEM_CHEMTORSION"
      schema="nmr" 
    inverse="true" 
    cascade="all"
  >
    <meta attribute="field-description">N/A</meta>
    <meta attribute="base-name">hbChemTorsion</meta>
    <cache usage="read-write"/>
    <key column="CHEMCOMPID"/>
    <one-to-many class="ccp.api.molecule.ChemComp.ChemTorsion"/>
  </set>
  
  <set 
    name="hbLinkEnds" 
    table="CHEM_LINKEND"
      schema="nmr" 
    inverse="true" 
    cascade="all"
  >
    <meta attribute="field-description">N/A</meta>
    <meta attribute="base-name">hbLinkEnd</meta>
    <cache usage="read-write"/>
    <key column="CHEMCOMPID"/>
    <one-to-many class="ccp.api.molecule.ChemComp.LinkEnd"/>
  </set>
  
  <set 
    name="hbNamingSystems" 
    table="CHEM_NAMINGSYSTEM"
      schema="nmr" 
    inverse="true" 
    cascade="all"
  >
    <meta attribute="field-description">N/A</meta>
    <meta attribute="base-name">hbNamingSystem</meta>
    <cache usage="read-write"/>
    <key column="CHEMCOMPID"/>
    <one-to-many class="ccp.api.molecule.ChemComp.NamingSystem"/>
  </set>
  
  <set 
    name="hbStereochemistries" 
    table="CHEM_STEREOCHEMISTRY"
      schema="nmr" 
    inverse="true" 
    cascade="all"
  >
    <meta attribute="field-description">N/A</meta>
    <meta attribute="base-name">hbStereochemistry</meta>
    <cache usage="read-write"/>
    <key column="CHEMCOMPID"/>
    <one-to-many class="ccp.api.molecule.ChemComp.Stereochemistry"/>
  </set>
  
  <set 
    name="hbResidueTypeProbs" 
    table="NMR_RESIDUETYPEPROB"
      schema="nmr" 
    inverse="true" 
    cascade="none"
  >
    <meta attribute="field-description">ResidueTypeProb describing ResonanceGroups with a probability of corresponding to this ChemComp.</meta>
    <meta attribute="base-name">hbResidueTypeProb</meta>
    <cache usage="read-write"/>
    <key column="POSSIBILITYID"/>
    <one-to-many class="ccp.api.nmr.Nmr.ResidueTypeProb"/>
  </set>

  </class>

</hibernate-mapping>
