<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping auto-import="false" package="ccp.api.nmr.Nmr">
  <class
    name="ResonanceGroup"
    table="NMR_RESONANCEGROUP"
    schema="nmr" 
  >
  <meta attribute="extends" inherit="false">memops.general.AbstractModelObject</meta>
  <meta attribute="implements">java.lang.Comparable</meta>
  <meta attribute="class-description">Group of Resonances belonging within the same Residue. Serves to organise Resonances that have not yet been assigned, and to set Residue types, (sequential) neighbours, Residues and their probabilities for an entire group without necesarily assigning the individual Resonances. Coherence between ResonanceGroup (probable) assignments and assignments of Resonances to Atoms are not checked by the API software. In case of conflicts the assignment of the Resonance should take precedence over any information carried by the ResonanceGroup, in accordance with the status of the ResonanceGroup as a vehicle for tentative assignments.</meta>
  <cache usage="read-write"/>

  <id
    name="_dbId"
    access="field"
    column="DATAOBJECTID"
    type="java.lang.Long">
    <meta attribute="scope-set">protected</meta>
    <generator class="native"/>
  </id>
  
  <many-to-one
    name="_topObject"
    access="field"
    column="TOPOBJECTID"
    class="ccp.api.nmr.Nmr.NmrProject"
    not-null="true"
    unique="false"
    foreign-key="NMR_RESOGROU_NMRPROJECT_TO_FK"
  >
    <meta attribute="field-description">hard coded link to topObject</meta>
  </many-to-one>
  
  <property
    name="_ccpCode"
    access="field"
    type="java.lang.String" length="80"
    not-null="false"
  >
    <meta attribute="field-description">ccpCode ('threeLetterCode') identifying the ChemComp giving the type of the ResonanceGroup. Used as a key in deriving the link chemComp.</meta>
    <meta attribute="default-value">null</meta>
    <column name="CCPCODE" sql-type="VARCHAR(80)" length="80" not-null="false"/>
  </property>
  
  <property
    name="_descriptor"
    access="field"
    type="java.lang.String" length="80"
    not-null="false"
  >
    <meta attribute="field-description">String describing the precise form of the ChemCompVar giving the type of the ResonanceGroup. Used as a key in deriving the link chemCompVar.</meta>
    <meta attribute="default-value">null</meta>
    <column name="DESCRIPTOR" sql-type="VARCHAR(80)" length="80" not-null="false"/>
  </property>
  
  <property
    name="_details"
    access="field"
    type="java.lang.String" length="4000"
    not-null="false"
  >
    <meta attribute="field-description">Free text, for notes, explanatory comments, etc.</meta>
    <meta attribute="default-value">null</meta>
    <column name="DETAILS" sql-type="VARCHAR(4000)" length="4000" not-null="false"/>
  </property>
  
  <property
    name="_linking"
    access="field"
    type="java.lang.String" length="32"
    not-null="false"
  >
    <meta attribute="field-description">String describing the linking of the ChemComp giving the type of the ResonanceGroup. Used as a key in deriving the link chemCompVar.</meta>
    <meta attribute="default-value">null</meta>
    <column name="LINKING" sql-type="VARCHAR(32)" length="32" not-null="false"/>
  </property>
  
  <property
    name="_molType"
    access="field"
    type="java.lang.String" length="32"
    not-null="false"
  >
    <meta attribute="field-description">String describing the molecule type of the ChemComp giving the type of the ResonanceGroup. Used as a key in deriving the link chemComp.</meta>
    <meta attribute="default-value">null</meta>
    <column name="MOLTYPE" sql-type="VARCHAR(32)" length="32" not-null="false"/>
  </property>
  
  <property
    name="_name"
    access="field"
    type="java.lang.String" length="80"
    not-null="false"
  >
    <meta attribute="field-description">Name of ResonanceGroup. Freely modifiable.</meta>
    <meta attribute="default-value">null</meta>
    <column name="NAME" sql-type="VARCHAR(80)" length="80" not-null="false"/>
  </property>
  
  <property
    name="_secStrucCode"
    access="field"
    type="java.lang.String" length="32"
    not-null="false"
  >
    <meta attribute="field-description">Code for secondary structure assigned to ResonanceGroup.</meta>
    <meta attribute="default-value">null</meta>
    <column name="SECSTRUCCODE" sql-type="VARCHAR(32)" length="32" not-null="false"/>
  </property>
  
  <property
    name="_serial"
    access="field"
    type="java.lang.Integer"
    not-null="true"
  >
    <meta attribute="field-description">Serial number of object. Serves as object main key. Serial numbers of deleted objects are not re-used. Serial numbers can only be set by the implementation. Values are in practice always positive, since negative values are interpreted as a signal to set the next free serial</meta>
    <meta attribute="default-value">null</meta>
    <column name="SERIAL" sql-type="INTEGER" not-null="true" unique-key="resonanceGroupUniqueKey" />
  </property>
  
  <list
    name="hbApplicationData"
    table="NMR_RESOGROU2APPLDATA"
      schema="nmr" 
    access="property"
    cascade="all"
  >
    <meta attribute="field-description">Application-specific data</meta>
    <key column="DATAOBJECTID"/>
    <list-index column="ORDER_applicationData_" base="1"/>
    <many-to-many
      column="APPLICATIONDATAID"
      class="memops.api.Implementation.ApplicationData"
      foreign-key="IMPL_DATAOBJECT_APPLDATA_FK"
      unique="true">
    </many-to-many>
  </list>
  
  <many-to-one
    name="hbAccess"
    column="ACCESSID"
    class="memops.api.AccessControl.AccessObject" 
    not-null="false"
    unique="true"
    foreign-key="IMPL_DATAOBJECT_ACCESS_FK" 
  >
    <meta attribute="field-description">Access object that controls object access</meta>
  </many-to-one>
  
  <many-to-one
    name="hbNmrProject"
    column="NMRPROJECTID"
    class="ccp.api.nmr.Nmr.NmrProject" 
    cascade="merge,save-update"
    not-null="true"
    unique="false"
    foreign-key="NMR_RESOGROU_NMRPROJECT_FK" 
    unique-key="resonanceGroupUniqueKey" 
  >
    <meta attribute="field-description">N/A</meta>
  </many-to-one>
  
  <many-to-one
    name="hbResidue"
    column="RESIDUEID"
    class="ccp.api.molecule.MolSystem.Residue" 
    cascade="merge,save-update"
    not-null="false"
    unique="false"
    foreign-key="NMR_RESOGROU_RESIDUE_FK" 
  >
    <meta attribute="field-description">Residue to which ResonanceGroup definitely belongs. In case of conflict, this link overrides conflicting ResidueType or Residue probabilities.</meta>
  </many-to-one>
  
  <set 
    name="hbFromResonanceGroups" 
    table="NMR_RESONANCEGROUPPROB"
      schema="nmr" 
    inverse="true" 
    cascade="none"
  >
    <meta attribute="field-description">ResonanceGroupProbs describing linking probabilities to this Resonance</meta>
    <meta attribute="base-name">hbFromResonanceGroup</meta>
    <cache usage="read-write"/>
    <key column="POSSIBILITYID"/>
    <one-to-many class="ccp.api.nmr.Nmr.ResonanceGroupProb"/>
  </set>
  
  <set 
    name="hbResidueProbs" 
    table="NMR_RESIDUEPROB"
      schema="nmr" 
    inverse="true" 
    cascade="all"
  >
    <meta attribute="field-description">ResidueProbs describing residues that probably correspond to the ResonanceGroup.</meta>
    <meta attribute="base-name">hbResidueProb</meta>
    <cache usage="read-write"/>
    <key column="RESONANCEGROUPID"/>
    <one-to-many class="ccp.api.nmr.Nmr.ResidueProb"/>
  </set>
  
  <set 
    name="hbResidueTypeProbs" 
    table="NMR_RESIDUETYPEPROB"
      schema="nmr" 
    inverse="true" 
    cascade="all"
  >
    <meta attribute="field-description">ResidueTypeProbs describing probable ChemComp types of ResonanceGroup</meta>
    <meta attribute="base-name">hbResidueTypeProb</meta>
    <cache usage="read-write"/>
    <key column="RESONANCEGROUPID"/>
    <one-to-many class="ccp.api.nmr.Nmr.ResidueTypeProb"/>
  </set>
  
  <set 
    name="hbResonanceGroupProbs" 
    table="NMR_RESONANCEGROUPPROB"
      schema="nmr" 
    inverse="true" 
    cascade="all"
  >
    <meta attribute="field-description">ResonanceGroupProbs describing probable links from (owned by) this ResonanceGroup.</meta>
    <meta attribute="base-name">hbResonanceGroupProb</meta>
    <cache usage="read-write"/>
    <key column="FROMRESONANCEGROUPID"/>
    <one-to-many class="ccp.api.nmr.Nmr.ResonanceGroupProb"/>
  </set>
  
  <set 
    name="hbResonanceProbs" 
    table="NMR_RESONANCEPROB"
      schema="nmr" 
    inverse="true" 
    cascade="none"
  >
    <meta attribute="field-description">ResonanceProbs describing Resonances that have a probability of belonging to this ResonanceGroup</meta>
    <meta attribute="base-name">hbResonanceProb</meta>
    <cache usage="read-write"/>
    <key column="POSSIBILITYID"/>
    <one-to-many class="ccp.api.nmr.Nmr.ResonanceProb"/>
  </set>
  
  <set 
    name="hbResonances" 
    table="NMR_RESONANCE"
      schema="nmr" 
    inverse="true" 
    cascade="none"
  >
    <meta attribute="field-description">Resonances belonging to ResonanceGroup. This link overrides any conflicting ResonanceProbs.</meta>
    <meta attribute="base-name">hbResonance</meta>
    <cache usage="read-write"/>
    <key column="RESONANCEGROUPID"/>
    <one-to-many class="ccp.api.nmr.Nmr.Resonance"/>
  </set>
  
  <set 
    name="hbChains" 
    table="NMR_RESOGROU2CHAINS"
      schema="nmr" 
    inverse="false" 
    cascade="none"
  >
    <meta attribute="field-description">Chains to which ResonanceGroup may belong</meta>
    <meta attribute="base-name">hbChain</meta>
    <cache usage="read-write"/>
    <key column="RESONANCEGROUPID" foreign-key="MOLS_CHAIN_RESOGROU_FK"/>
    <many-to-many column="CHAINID" class="ccp.api.molecule.MolSystem.Chain" foreign-key="NMR_RESOGROU_CHAINS_FK"/>
  </set>

  </class>

</hibernate-mapping>
