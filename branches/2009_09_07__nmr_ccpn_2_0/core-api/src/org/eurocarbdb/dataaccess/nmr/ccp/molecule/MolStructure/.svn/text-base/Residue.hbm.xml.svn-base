<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping auto-import="false" package="ccp.api.molecule.MolStructure">
  <class
    name="Residue"
    table="COOR_RESIDUE"
    schema="nmr" 
  >
  <meta attribute="extends" inherit="false">memops.general.AbstractModelObject</meta>
  <meta attribute="implements">java.lang.Comparable</meta>
  <meta attribute="class-description">Residue in Molsystem Coordinate description</meta>
  <cache usage="read-write"/>

  <id
    name="_dbId"
    access="field"
    column="DATAOBJECTID"
    type="java.lang.Long">
    <meta attribute="scope-set">protected</meta>
    <generator class="native"/>
  </id>
  
  <many-to-one
    name="_topObject"
    access="field"
    column="TOPOBJECTID"
    class="ccp.api.molecule.MolStructure.StructureEnsemble"
    not-null="true"
    unique="false"
    foreign-key="COOR_RESIDUE_STRUENSE_TO_FK"
  >
    <meta attribute="field-description">hard coded link to topObject</meta>
  </many-to-one>
  
  <property
    name="_seqCode"
    access="field"
    type="java.lang.Integer"
    not-null="true"
  >
    <meta attribute="field-description">Molecular sequence number as used in Coordinate description. </meta>
    <meta attribute="default-value">null</meta>
    <column name="SEQCODE" sql-type="INTEGER" not-null="true"/>
  </property>
  
  <property
    name="_seqId"
    access="field"
    type="java.lang.Integer"
    not-null="true"
  >
    <meta attribute="field-description">Identifier corresponding to the molResidue identifier (self.residue.molResidue.serial)</meta>
    <meta attribute="default-value">null</meta>
    <column name="SEQID" sql-type="INTEGER" not-null="true" unique-key="residueUniqueKey" />
  </property>
  
  <property
    name="_seqInsertCode"
    access="field"
    type="java.lang.String" length="80"
    not-null="true"
  >
    <meta attribute="field-description">Molecular sequence insert code as used in Coordinate description.</meta>
    <meta attribute="default-value">" "</meta>
    <column name="SEQINSERTCODE" sql-type="VARCHAR(80)" length="80" not-null="true"/>
  </property>
  
  <list
    name="hbApplicationData"
    table="COOR_RESIDUE2APPLDATA"
      schema="nmr" 
    access="property"
    cascade="all"
  >
    <meta attribute="field-description">Application-specific data</meta>
    <key column="DATAOBJECTID"/>
    <list-index column="ORDER_applicationData_" base="1"/>
    <many-to-many
      column="APPLICATIONDATAID"
      class="memops.api.Implementation.ApplicationData"
      foreign-key="IMPL_DATAOBJECT_APPLDATA_FK"
      unique="true">
    </many-to-many>
  </list>
  
  <many-to-one
    name="hbAccess"
    column="ACCESSID"
    class="memops.api.AccessControl.AccessObject" 
    not-null="false"
    unique="true"
    foreign-key="IMPL_DATAOBJECT_ACCESS_FK" 
  >
    <meta attribute="field-description">Access object that controls object access</meta>
  </many-to-one>
  
  <many-to-one
    name="hbChain"
    column="CHAINID"
    class="ccp.api.molecule.MolStructure.Chain" 
    cascade="merge,save-update"
    not-null="true"
    unique="false"
    foreign-key="COOR_RESIDUE_CHAIN_FK" 
    unique-key="residueUniqueKey" 
  >
    <meta attribute="field-description">N/A</meta>
  </many-to-one>
  
  <set 
    name="hbAtoms" 
    table="COOR_ATOM"
      schema="nmr" 
    inverse="true" 
    cascade="all"
  >
    <meta attribute="field-description">N/A</meta>
    <meta attribute="base-name">hbAtom</meta>
    <cache usage="read-write"/>
    <key column="RESIDUEID"/>
    <one-to-many class="ccp.api.molecule.MolStructure.Atom"/>
  </set>
  
  <set 
    name="hbResidueValidations" 
    table="VALD_RESIVALI2RESIDUES"
      schema="nmr" 
    inverse="true" 
    cascade="none"
  >
    <meta attribute="field-description">Validation results that include Residue</meta>
    <meta attribute="base-name">hbResidueValidation</meta>
    <cache usage="read-write"/>
    <key column="RESIDUEID" foreign-key="VALD_RESIVALI_RESIDUES_FK"/>
    <many-to-many column="RESIDUEVALIDATIONID" class="ccp.api.molecule.Validation.ResidueValidation" foreign-key="COOR_RESIDUE_RESIVALI_FK"/>
  </set>

  </class>

</hibernate-mapping>
