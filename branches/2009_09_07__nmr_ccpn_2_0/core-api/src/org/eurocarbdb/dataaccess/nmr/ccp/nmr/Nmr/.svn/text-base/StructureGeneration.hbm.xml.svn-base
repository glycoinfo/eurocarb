<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping auto-import="false" package="ccp.api.nmr.Nmr">
  <class
    name="StructureGeneration"
    table="NMR_STRUCTUREGENERATION"
    schema="nmr" 
  >
  <meta attribute="extends" inherit="false">memops.general.AbstractModelObject</meta>
  <meta attribute="implements">java.lang.Comparable</meta>
  <meta attribute="class-description">One cycle of Nmr Structure generation, with associated constraints, protocol, parameters, etc., producing one family of structures. NB protocol etc. information still not modeled. </meta>
  <cache usage="read-write"/>

  <id
    name="_dbId"
    access="field"
    column="DATAOBJECTID"
    type="java.lang.Long">
    <meta attribute="scope-set">protected</meta>
    <generator class="native"/>
  </id>
  
  <many-to-one
    name="_topObject"
    access="field"
    column="TOPOBJECTID"
    class="ccp.api.nmr.Nmr.NmrProject"
    not-null="true"
    unique="false"
    foreign-key="NMR_STRUGENE_NMRPROJECT_TO_FK"
  >
    <meta attribute="field-description">hard coded link to topObject</meta>
  </many-to-one>
  
  <property
    name="_details"
    access="field"
    type="java.lang.String" length="254"
    not-null="false"
  >
    <meta attribute="field-description">Free text, for notes, explanatory comments, etc.</meta>
    <meta attribute="default-value">null</meta>
    <column name="DETAILS" sql-type="VARCHAR(254)" length="254" not-null="false"/>
  </property>
  
  <property
    name="_generationType"
    access="field"
    type="java.lang.String" length="32"
    not-null="true"
  >
    <meta attribute="field-description">Generation type (denovo, refinement).</meta>
    <meta attribute="default-value">"denovo"</meta>
    <column name="GENERATIONTYPE" sql-type="VARCHAR(32)" length="32" not-null="true"/>
  </property>
  
  <property
    name="_name"
    access="field"
    type="java.lang.String" length="80"
    not-null="false"
  >
    <meta attribute="field-description">StructureGeneration name - to specify more precisely the nature of the particular structure generation.</meta>
    <meta attribute="default-value">null</meta>
    <column name="NAME" sql-type="VARCHAR(80)" length="80" not-null="false"/>
  </property>
  
  <property
    name="_serial"
    access="field"
    type="java.lang.Integer"
    not-null="true"
  >
    <meta attribute="field-description">Serial number of object. Serves as object main key. Serial numbers of deleted objects are not re-used. Serial numbers can only be set by the implementation. Values are in practice always positive, since negative values are interpreted as a signal to set the next free serial</meta>
    <meta attribute="default-value">null</meta>
    <column name="SERIAL" sql-type="INTEGER" not-null="true" unique-key="structureGenerationUniqueKey" />
  </property>
  
  <list
    name="hbApplicationData"
    table="NMR_STRUGENE2APPLDATA"
      schema="nmr" 
    access="property"
    cascade="all"
  >
    <meta attribute="field-description">Application-specific data</meta>
    <key column="DATAOBJECTID"/>
    <list-index column="ORDER_applicationData_" base="1"/>
    <many-to-many
      column="APPLICATIONDATAID"
      class="memops.api.Implementation.ApplicationData"
      foreign-key="IMPL_DATAOBJECT_APPLDATA_FK"
      unique="true">
    </many-to-many>
  </list>
  
  <many-to-one
    name="hbAccess"
    column="ACCESSID"
    class="memops.api.AccessControl.AccessObject" 
    not-null="false"
    unique="true"
    foreign-key="IMPL_DATAOBJECT_ACCESS_FK" 
  >
    <meta attribute="field-description">Access object that controls object access</meta>
  </many-to-one>
  
  <many-to-one
    name="hbMethod"
    column="METHODID"
    class="ccp.api.general.Method.Method" 
    cascade="merge,save-update"
    not-null="false"
    unique="false"
    foreign-key="NMR_STRUGENE_METHOD_FK" 
  >
    <meta attribute="field-description">Method used for generating structures</meta>
  </many-to-one>
  
  <many-to-one
    name="hbNmrProject"
    column="NMRPROJECTID"
    class="ccp.api.nmr.Nmr.NmrProject" 
    cascade="merge,save-update"
    not-null="true"
    unique="false"
    foreign-key="NMR_STRUGENE_NMRPROJECT_FK" 
    unique-key="structureGenerationUniqueKey" 
  >
    <meta attribute="field-description">N/A</meta>
  </many-to-one>
  
  <many-to-one
    name="hbStructureEnsemble"
    column="STRUCTUREENSEMBLEID"
    class="ccp.api.molecule.MolStructure.StructureEnsemble" 
    not-null="false"
    unique="true"
    foreign-key="NMR_STRUGENE_STRUENSE_FK" 
  >
    <meta attribute="field-description">StructureEnsemble generated during StructureGeneration</meta>
  </many-to-one>
  
  <many-to-one
    name="hbNmrConstraintStore"
    column="NMRCONSTRAINTSTOREID"
    class="ccp.api.nmr.NmrConstraint.NmrConstraintStore" 
    cascade="merge,save-update"
    not-null="false"
    unique="false"
    foreign-key="NMR_STRUGENE_NMRCONSSTOR_FK" 
  >
    <meta attribute="field-description">NmrConstraintStore holding constraints used in StructureGeneration</meta>
  </many-to-one>
  
  <set 
    name="hbEntries" 
    table="ENTR_ENTRY2STRUGENE"
      schema="nmr" 
    inverse="true" 
    cascade="none"
  >
    <meta attribute="field-description">Deposition Entries for StructureGeneration</meta>
    <meta attribute="base-name">hbEntry</meta>
    <cache usage="read-write"/>
    <key column="STRUCTUREGENERATIONID" foreign-key="ENTR_ENTRY_STRUGENE_FK"/>
    <many-to-many column="ENTRYID" class="ccp.api.nmr.NmrEntry.Entry" foreign-key="NMR_STRUGENE_ENTRIES_FK"/>
  </set>

  </class>

</hibernate-mapping>
