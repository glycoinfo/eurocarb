        <TD VALIGN=TOP WIDTH='100%' HEIGHT=10><H1>CCP:RefExperimentPopup</H1></TD>
        <TD ROWSPAN=3>&nbsp;</TD>
        <TD VALIGN=TOP ROWSPAN=3>
          <TABLE BORDER=0 CELLPADDING=0 CELLSPACING=0>
          <TR><TD BGCOLOR='#FFFFFF'><TABLE BORDER=0 CELLPADDING=2 CELLSPACING=1 WIDTH="100%" VALIGN=TOP>
<TR><TD CLASS="section"><NOBR>Popups:</NOBR> <NOBR>CCP</NOBR></TD></TR><TR><TD CLASS="pagesec" ><A CLASS="menu" HREF="NmrExpPrototypePopup.html">NmrExpPrototypePopup</A></TD></TR>
<TR><TD CLASS="pagesec" ><A CLASS="menu" HREF="ExpGraphPopup.html">ExpGraphPopup</A></TD></TR>
<TR><TD CLASS="pagesel" ><A CLASS="menuSel" HREF="RefExperimentPopup.html">RefExperimentPopup</A></TD></TR>
<TR><TD CLASS="pagesec" ><A CLASS="menu" HREF="NmrExpPrototypeNomenclature.html">NmrExpPrototypeNomenclature</A></TD></TR>
<TR><TD CLASS="pagesec" ><A CLASS="menu" HREF="SaveProjectAs.html">SaveProjectAs</A></TD></TR>
<TR><TD></TD></TR></TABLE></TD><TR></TABLE>
        </TD>
