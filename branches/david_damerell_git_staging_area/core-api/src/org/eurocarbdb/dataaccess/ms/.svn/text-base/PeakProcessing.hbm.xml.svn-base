<hibernate-mapping default-cascade="save-update">

    <class name="org.eurocarbdb.dataaccess.ms.PeakProcessing" table="peak_processing" schema="ms">
        <id name="peakProcessingId" type="int">
            <column name="peak_processing_id" />
