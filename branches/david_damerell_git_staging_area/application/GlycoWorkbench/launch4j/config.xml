<launch4jConfig>
  <headerType>gui</headerType>
  <outfile>../dist/GlycoWorkbench.exe</outfile>
  <jar>GlycoWorkbench.jar</jar>
  <dontWrapJar>true</dontWrapJar>
  <errTitle>GlycoWorkbench: startup</errTitle>
  <downloadUrl>http://java.com/download</downloadUrl>
  <supportUrl>mailto:a.ceroni@imperial.ac.uk</supportUrl>
  <priority>normal</priority>
  <customProcName>false</customProcName>
  <stayAlive>false</stayAlive>
  <icon>GlycoWorkbench.ico</icon>
  <classPath>
    <mainClass>org.eurocarbdb.application.glycoworkbench.GlycoWorkbench</mainClass>
    <cp>libs/*.jar</cp>
  </classPath>
  <jre>
    <minVersion>1.5.0</minVersion>
    <initialHeapSize>128</initialHeapSize>
    <maxHeapSize>512</maxHeapSize>   
  </jre>
  <splash>
    <file>GlycoWorkbench.bmp</file>
    <waitForWindow>true</waitForWindow>
    <timeout>60</timeout>
    <timeoutErr>false</timeoutErr>
  </splash>
  <versionInfo>
    <fileVersion>1.0.3353.0</fileVersion>
    <txtFileVersion>1.0.3353</txtFileVersion>
    <fileDescription>GlycoWorkbench</fileDescription>
    <copyright>LGPL</copyright>
    <productVersion>1.0.3353.0</productVersion>
    <txtProductVersion>1.0.3353</txtProductVersion>
    <productName>GlycoWorkbench</productName>
    <companyName>EUROCarbDB</companyName>
    <internalName>GlycoWorkbench</internalName>
    <originalFilename>GlycoWorkbench.exe</originalFilename>
  </versionInfo>
</launch4jConfig>
