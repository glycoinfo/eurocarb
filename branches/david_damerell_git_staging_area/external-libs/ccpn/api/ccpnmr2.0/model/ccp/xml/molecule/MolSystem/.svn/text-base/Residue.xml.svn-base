ccp.api.molecule.Molecule.MolResidue molResidue = getChain().getMolecule().findFirstMolResidue("serial", getSeqId());
ccp.api.molecule.ChemComp.ChemCompVar chemCompVar = molResidue.getChemComp().findFirstChemCompVar("linking",getLinking(),"descriptor",getDescriptor());

java.util.Set&lt;java.lang.String&gt; linkCodes = new java.util.HashSet&lt;java.lang.String&gt;();
for (ccp.api.molecule.ChemComp.LinkEnd linkEnd: chemCompVar.getLinkEnds())
