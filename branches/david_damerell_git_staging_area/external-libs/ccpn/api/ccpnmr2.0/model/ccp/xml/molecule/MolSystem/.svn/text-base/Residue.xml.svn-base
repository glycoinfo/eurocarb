  if (!linkCodes.contains(linkEnd.getLinkCode()))
    isValid = false;
}

for (ccp.api.molecule.Molecule.MolResLinkEnd linkEnd: getMolResidue().getMolResLinkEnds()) {
  if (!linkCodes.contains(linkEnd.getLinkCode()))
    isValid = false;
}

java.util.Set&lt;java.lang.String&gt; dd = new java.util.HashSet&lt;java.lang.String&gt;();
for (ccp.api.molecule.ChemComp.AbstractChemAtom ca: chemCompVar.getChemAtoms()) {
  if (ca instanceof ccp.api.molecule.ChemComp.ChemAtom)
    dd.add(((ccp.api.molecule.ChemComp.ChemAtom) ca).getName());
}

for (ccp.api.molecule.MolSystem.Atom ma: getAtoms()) {
  if (!dd.contains(ma.getName()))
    isValid = false;
}
