      <item tag="java">java.lang.String linking = getLinking();
java.lang.String descriptor = getDescriptor();
isValid = true;

if ("any".equals(linking) &amp;&amp; "any".equals(descriptor))
{
  int nVars = getChemCompNmrRef().getChemCompVarNmrRefs().size();
  for (ccp.api.nmr.NmrReference.ChemAtomNmrRef atom: getChemAtomNmrRefs()) {
    if (atom.getChemCompVarNmrRefs().size() != nVars) {
      isValid = false;
      break;
    }
  }
} else if  ("any".equals(linking) ) {
  java.util.Collection otherVars = getChemCompNmrRef().findAllChemCompVarNmrRefs("descriptor",descriptor);
  otherVars.remove(this);
  for (ccp.api.nmr.NmrReference.ChemAtomNmrRef atom: getChemAtomNmrRefs()) {
    java.util.Collection ll = atom.getChemCompVarNmrRefs();
    if (!ll.containsAll(otherVars)) {
      isValid = false;
      break;
    }
  }
} else if ("any".equals(descriptor) ) {
  java.util.Collection otherVars = getChemCompNmrRef().findAllChemCompVarNmrRefs("linking",linking);
  otherVars.remove(this);
  for (ccp.api.nmr.NmrReference.ChemAtomNmrRef atom: getChemAtomNmrRefs()) {
    java.util.Collection ll = atom.getChemCompVarNmrRefs();
    if (!ll.containsAll(otherVars)) {
      isValid = false;
      break;
    }
  }
}
