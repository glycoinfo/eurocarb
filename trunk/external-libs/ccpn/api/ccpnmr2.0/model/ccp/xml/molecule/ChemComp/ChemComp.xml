    <codeStubs>
      <item tag="java">int lin = 0;
int nonlin = 0;

for (ccp.api.molecule.ChemComp.ChemCompVar chemCompVar: getChemCompVars()) {
  String linking = chemCompVar.getLinking();
  if ( ("start".equals(linking)) || ("middle".equals(linking)) || ("end".equals(linking)) ) {
    lin = lin + 1;
  } else if (!("none".equals(linking))) {
    nonlin = nonlin + 1;
  }
}

if (lin != 0) {
  if (nonlin != 0) {
    throw new memops.general.ApiException("ChemComp " + getMolType() + "," + getCcpCode() + " has illegal combination of ChemCompVar linkings");
  } else {
    result = true;
  }
} else {
  result = false;
}</item>
      <item tag="python">lin = 0
