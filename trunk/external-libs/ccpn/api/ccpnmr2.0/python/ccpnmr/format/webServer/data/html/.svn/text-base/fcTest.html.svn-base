<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
<head>
    <meta http-equiv="content-type" content="text/html; charset=us-ascii">
    <title>CcpNmr FormatConverter Home Page</title>
    <style type="text/css" media="screen">@import 'css/fc.css';</style>
    <base target="_blank">
</head>

<body scrolling="no">
<form method="POST" action="">
<table  style="WIDTH: 100%" border="0" cellspacing="0" cellpadding="0">
  <tr bgcolor="white" height="120">
    <td colspan="2"  align="left" ><img src="images/logo.png" alt="logo" border-width="0"></td>
  </tr>
  <tr>
    <td colspan="2" height="1" bgcolor="#afafcf"></td>
  </tr>
  <tr>
    <td colspan="2" class="mainMenu">
      <a href="index" class="mainMenuItem">Start</a>&nbsp;&nbsp;|&nbsp;&nbsp; 
      <a href="../fcLinks" class="mainMenuItem">Links</a>&nbsp;&nbsp;|&nbsp;&nbsp;
      <a href="../fcReference" class="mainMenuItem">Reference</a>
  </td>
  </tr>
  <tr>
    <td colspan="2" height="1" bgcolor="#afafcf"></td>
  </tr>

  <tr style="height: 30px">
    <td colspan="2" class="pageTitle"  valign="bottom">Page title</td>
  </tr>

  <tr>
    <td colspan="2" height="1" bgcolor="#afafcf"></td>
  </tr>
  <tr>
    <td colspan="1" width="100px" class="pageTitle"> Page description</td>
    <td colspan="1">
    <% rows = [range(0,10)] %>
    <table align="center" style="WIDTH: 50%" border="1" cellspacing="0" cellpadding="5" bgcolor="#dedbff">
      % for row in rows:
        ${makerow(row)}
      % endfor
    </table>

    <%def name="makerow(row)">
      % for name in row:
      <tr >
        <td align="center"><font class="parameterItem"> parameter name </font></td>
        <td align="center"><input type="radio" name="group${name}" value="On"> On <input type="radio" name="group${name}" value="off" checked> off</td>
        <td align="center">Description<img src="images/i_button_small.png" title="Parameter tooltip help"></td>
      </tr>
      % endfor
    </%def>
    </td>
  </tr>
      <tr bgcolor="white" border="0">
        <td></td>
        <td align="center"><input type="submit" value="Submit"></td>
      </tr>
</table>
</form>
</body>
</html>
